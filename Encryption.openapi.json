openapi: 3.0.0
info:
  title: Encryption
  version: 1.0.0
servers:
  - url: https://o2x4s7fcyl.execute-api.us-east-1.amazonaws.com
paths:
  /dev/encrypt:
    get:
      tags:
        - default
      summary: Encrypt
      responses:
        '200':
          description: OK
          headers:
            Date:
              schema:
                type: string
                example: Sat, 11 Feb 2023 16:50:38 GMT
            Content-Type:
              schema:
                type: string
                example: application/json
            Content-Length:
              schema:
                type: integer
                example: '140'
            Connection:
              schema:
                type: string
                example: close
            x-amzn-RequestId:
              schema:
                type: string
                example: 9611d68e-8539-4125-99cc-efbecd8032b5
            x-amz-apigw-id:
              schema:
                type: string
                example: ALwV7EnPoAMFWzg=
            X-Amzn-Trace-Id:
              schema:
                type: string
                example: Root=1-63e7c758-3b89f2dd565a62ba25bf8593;Sampled=0
          content:
            application/json:
              schema:
                type: object
              example:
                responseCode: '00'
                responseMessage: Success
                encryptedText: >-
                  DAAAAJLQ7JvgE+6ZSx/x0xAAAAD8Vik1r3DEOmaNCSqs8fn7ntA4KPDQdLcOPkB+VN1ygA==
  /dev/decrypt:
    get:
      tags:
        - default
      summary: Decrypt
      responses:
        '200':
          description: OK
          headers:
            Date:
              schema:
                type: string
                example: Sat, 11 Feb 2023 16:51:50 GMT
            Content-Type:
              schema:
                type: string
                example: application/json
            Content-Length:
              schema:
                type: integer
                example: '16'
            Connection:
              schema:
                type: string
                example: keep-alive
            x-amzn-RequestId:
              schema:
                type: string
                example: eb5315cb-d8b4-48be-8af8-447c72862fb0
            x-amz-apigw-id:
              schema:
                type: string
                example: ALwhlGC5IAMFsaA=
            X-Amzn-Trace-Id:
              schema:
                type: string
                example: Root=1-63e7c7a3-5598673d0481ce53640151c1;Sampled=0
          content:
            application/json:
              schema:
                type: object
              example:
                bankId: 1
